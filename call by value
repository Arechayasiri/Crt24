#include <stdio.h>

    void swap(int x,int y)
    {
        int t;
        t=x;
        x=y;
        y=t;
    }
        int main()
        {
            int a,b;
            printf("Enter two numbers");
            scanf("%d%d",&a,&b);
            printf("values before swapping a=%d b=%d");
            swap(a,b);
            printf("value after swaping a=%d b=%d");
        }
